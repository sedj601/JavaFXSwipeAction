<AnchorPane id="AnchorPane" prefHeight="508.0" prefWidth="644.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="swipeaction.FXMLDocumentController">
